##用户列表
#parse("admin/navigation-head.vm")
<div class="portlet box green-haze users-list">
	<div class="portlet-title">
		<div class="caption">
			<i class="fa fa-globe"></i>在线视频
		</div>
		<div class="tools">
			<a href="javascript:;" onclick="familyVideo.reload()" class="reload"> </a>
		</div>
	</div>
	<div class="portlet-body">
		<div class="table-toolbar">
			<div class="row">
				<div class="col-md-6">
					<div class="btn-group">
						<button class="btn green" onclick="familyVideo.edit()">
							编辑在线视频<i class="fa fa-plus"></i>
						</button>
					</div>
				</div>
				<div class="col-md-6"></div>
			</div>
			<div class="row">
				<div class="col-md-6">&nbsp;</div>
			</div>
			<table class="table table-striped table-bordered table-hover" id="familyVideoTable">
		<thead>
			<tr role="row">
				<th class="numeric">
    				视频编号
    			</th>
    			<th class="numeric">
    				  视频封面
    			</th>
    			<th class="numeric">
    				 视频网址
    			</th>
    			
    			<th class="numeric">
    				 视频标题
    			</th>
<!--     			<th class="numeric"> -->
<!--     				 视频来源 -->
<!--     			</th> -->
    			<th class="numeric">
    				 事件类别
    			</th>
    			<th class="numeric">
    				视频上传日期
    			</th>
    			<th class="numeric">
    				视频编辑人
    			</th>
    			<th class="numeric">
    				修改
    			</th>
    			<th class="numeric">
    				删除
    			</th>
    		</tr>
		</thead>
		</table>
		</div>
		
	</div>
</div>
##编辑器
<div class="add-UEditor-familyVideo-list" >
##parse("admin/menus/set/familyVideo/familyVideo-add.vm")
</div>	
