##新增用户
<input type="hidden" id="uid"/>
<div class="row">
	<div class="col-md-12">
		<div id="add-bind-userInfo-form" class="modal modal-scroll" tabindex="-1" data-replace="true">
        	<div class="modal-header " >
        		<button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
        		<h4 class="modal-title">用户表单</h4>
        	</div>
			##模态框内容START
			<div class="modal-body">
            	<div class="portlet light bordered">
					##表单标题START
            		<div class="portlet-title">
            			<div class="caption font-green-haze">
            				<i class="icon-settings font-green-haze"></i>
            				<span class="caption-subject bold uppercase" id="user_title_table">家族成员信息</span>
            			</div>
            			<div class="actions">
            			<button type="button" class="btn blue" onClick="USERSLOGIN.addBindUserInfos('usersTable_Login')">提交<i class="fa fa-plus"></i></button>
            				<a class="btn btn-circle btn-icon-only btn-default fullscreen" href="javascript:;" data-original-title="" title="">
            				</a>
            			</div>
            		</div>
					##表单标题END
					##表单内容START

            		<div class="portlet-body form">
           					<table class="table table-striped table-bordered table-hover" id="usersTable_Login">
								<thead>
									<tr role="row">
										<th style="width:2%">
						    				单选
						    			</th>
						    			<th class="numeric">
						    				 用户编号
						    			</th>
						    			<th class="numeric">
						    				 用户名称
						    			</th>
										<th class="numeric">
						    				性别
						    			</th>
										<th class="numeric">
						    				联系电话
						    			</th>
						    			<th class="numeric">
						    				创建的时间
						    			</th>
						    			
						    		</tr>
								</thead>
								</table>
    				</div>
					##表单内容END
					##表单页脚START
                	##<div class="modal-footer">
                		
                    	##</div>
                	</div>
					##表单页脚END
				</div>
				
			</div>
			##模态框内容END
		</div>
	</div>
