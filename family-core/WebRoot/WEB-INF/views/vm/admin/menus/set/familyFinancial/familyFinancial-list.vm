##财务列表
#parse("admin/navigation-head.vm")
<div class="portlet box green-haze users-list">
	<div class="portlet-title">
		<div class="caption">
			<i class="fa fa-globe"></i>财务列表
		</div>
		<div class="tools">
			<a href="javascript:;" onclick="familyFinancial.reload()" class="reload"> </a>
		</div>
	</div>
	<div class="portlet-body">
		<div class="table-toolbar">
			<div class="row">
				<div class="col-md-6">
					<div class="btn-group">
						<button class="btn green" onclick="familyFinancial.edit()">
							编辑<i class="fa fa-plus"></i>
						</button>
					</div>
				</div>
				<div class="col-md-6"></div>
			</div>
			<div class="row">
				<div class="col-md-6">&nbsp;</div>
			</div>
			<table class="table table-striped table-bordered table-hover" id="familyFinancialTable">
		<thead>
			<tr role="row">
				<th class="numeric">
    				 财务编号
    			</th>
    			<th class="numeric">
    				 财务HTML内容
    			</th>
    			<th class="numeric">
    				 事件别名
    			</th>
    			<th class="numeric">
    				 财务标题
    			</th>
    			<th class="numeric">
    				 财务内容
    			</th>
    			<th class="numeric">
    				发布账号/姓名
    			</th>
    			<th class="numeric">
    				最后修改时间
    			</th>
    			<th class="numeric">
    				查看
    			</th>
    			<th class="numeric">
    				删除
    			</th>
    			<th class="numeric">
    				置顶
    			</th>
    		</tr>
		</thead>
		</table>
		</div>
		
	</div>
</div>
##编辑器
<div class="add-UEditor-familyFinancial-list" >
##parse("admin/menus/set/familyFinancial/familyFinancial-add.vm")
</div>	
